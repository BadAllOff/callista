<h1><%= t('titles.services') %></h1>
<% if user_signed_in? %>
  <%= link_to 'New Service', new_service_path, class: 'btn btn-default' %>
<% end %>
<br>

<div class="row">
  <% @services.each do |service| %>
    <%= render partial: 'services/service', locals: { service: service } %>
  <% end %>

</div>